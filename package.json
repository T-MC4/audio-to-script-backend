{
    "name": "magical-script-generator-api-server",
    "version": "1.0.0",
    "description": "Generates sales scripts",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "build": "rimraf dist && npx tsc",
        "start": "NODE_ENV=production node dist/index.js",
        "preserve": "npm run build",
        "serve": "concurrently \"npx tsc -w\"  \"nodemon dist/index.js\""
    },
    "dependencies": {
        "@deepgram/sdk": "^2.3.0",
        "@supabase/supabase-js": "^2.31.0",
        "axios": "^1.4.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "langchain": "^0.0.95",
        "multer": "1.4.5-lts.1",
        "node-fetch": "^3.3.1",
        "tiktoken": "^1.0.10",
        "typeorm": "^0.3.17"
    },
    "engines": {
        "node": "18"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.17.3",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "concurrently": "^8.2.0",
        "eslint": "^8.46.0",
        "nodemon": "^3.0.1",
        "rimraf": "^5.0.1",
        "typescript": "^5.1.6"
    }
}
